{
  "version": 2,
  "builds": [
    { "src": "back-end/my-satify-app/api/**/*.ts", "use": "@vercel/node" },
    { "src": "font-end/my-satify-app/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "back-end/my-satify-app/api/index.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
